@model List<string>
@{ 
    var pageName = ViewContext.RouteData.Values.ContainsKey("action") ?
        ViewContext.RouteData.Values["action"].ToString().Replace("_", "") :
        ViewContext.RouteData.Values.ContainsKey("page") ?
        ViewContext.RouteData.Values["page"].ToString().Split("/").Last() :
        null;
    Model.Add(pageName.Substring(0, 1).ToLowerInvariant() + pageName.Substring(1));
}
<partial name="_Libs" model='Model' />