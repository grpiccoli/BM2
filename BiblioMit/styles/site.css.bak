/* Please see documentation at https://docs.microsoft.com/aspnet/core/client-side/bundling-and-minification\ 
for details on configuring this project to bundle and minify static web assets. */
body {
    padding-top: 4.3rem;
    /*min-height: 74rem;*/
    /*padding-bottom: 20px;*/
}

/* Wrapping element */
/* Set some basic padding to keep content from hitting the edges */

/* Carousel */
.carousel-caption p {
    font-size: 20px;
    line-height: 1.4;
}

/* Make .svg files in the carousel display properly in older browsers */
.carousel-inner .item img {
    width: 100%;
}

/* QR code generator */
#qrCode {
    margin: 15px;
}

/* Hide/rearrange for smaller screens */
@media screen and (max-width: 767px) {
    /* Hide captions */
    .carousel-caption {
        display: none;
    }
}

nav figure {
    margin-bottom: 0;
}

nav figure img {
    height: 35px;
    filter: sepia(65%) blur(0.2px);
}

nav .nav-item:hover .light-caption {
    color: azure;
    transition: color 0.5s linear;
}

.nav-item{
    min-width: 85px;
}

nav .nav-item:hover img {
    filter: sepia(0%) blur(0px);
    transition: filter 0.5s linear;
}

nav:hover .light-caption {
    visibility: visible;
    opacity: 1;
    transition: visibility 0s, opacity 0.5s linear;
}

.flag-icon {
    font-size: xx-large;
    border-radius: 50%;
}

.light-caption {
    visibility: hidden;
    opacity: 0;
    font-family: Tahoma;
    color: lightgray;
    font-size: small;
    transition: visibility 0.5s, opacity 0.5s linear;
    min-width: 85px;
}

nav .dropdown > .dropdown-menu{
    opacity: 0;
    margin: 0;
}

nav .dropdown:hover > .dropdown-menu {
    display: block;
    opacity: 1;
    transition: display 0.5s, opacity 0.5s linear;
}

.border-0{
    padding:0;
}

#navbarApps ul {
    margin: 0;
    padding: 0;
}

#selectLanguage .flag-icon-cl {
    float: left;
    clear: left;
}

#selectLanguage .text {
    float: left;
    clear: left;
}

#selectLanguage .dropdown-menu > li > a {
    padding: 10px;
}

#navbarApps .nav-link {
    width: 10vw;
    padding: 0;
    min-width: 85px;
}

.navbar {
    padding-bottom: 0;
}

