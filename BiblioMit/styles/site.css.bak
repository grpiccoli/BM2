.logo {
    align-content: center;
    display: inline-block;
    overflow: hidden;
    width: 1.5em;
    height: 1.5em;
    border-radius: 50px;
    padding: .23em .23em;
    color: white;
    font-size: x-large;
    box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.25);
}

    .logo:active {
        box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.25);
        left: 0;
        top: 0;
        transition: .5s ease-in-out;
    }

